css



function makeImageHappy(image) {
  image.src =
    "https://www.iconfinder.com/data/icons/streamline-emoji-1/48/013-smiling-face-with-heart-eyes-512.png";
}

function makeImageSad(image) {
  image.src =
    "https://www.iconfinder.com/data/icons/streamline-emoji-1/48/060-weary-face-1-512.png";
}




